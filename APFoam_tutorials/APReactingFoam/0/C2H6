FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object       C2H6;
} 
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * // 
 
dimensions      [0 0 0 0 0 0 0]; 
 
 
internalField   uniform  0;
 
boundaryField 
{ 
    source
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            inletOutlet; 
        inletValue      $internalField;
        value           $internalField;
    }
    inlet
    {
        type            fixedValue; 
        value           $internalField;
    }
    left
    {
        type            symmetryPlane;
    }
    top
    {
        type            symmetryPlane;
    }
    ground
    {
        type            zeroGradient;
    }
    right
    {
        type            symmetryPlane;
    }


}


